  `dcto` tinyint(4) NOT NULL DEFAULT '0',
  `archivos` varchar(300) DEFAULT NULL,
  `codsector` int(6) NOT NULL,
  PRIMARY KEY (`codpresupuesto`,`numlinea`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

